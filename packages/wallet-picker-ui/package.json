{
  "name": "@meer-wallet-selector/wallet-picker-ui",
  "version": "9.9.22",
  "description": "Wallet picker using a React Portal.",
  "scripts": {
    "compile:esm": "tsc -p tsconfig.esm.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "yarn rimraf dist node_modules package.tgz package",
    "publish": "yarn npm publish --access public --tolerate-republish"
  },
  "types": "./dist/esm/wallet-picker-ui/src/index.d.ts",
  "main": "./dist/cjs/wallet-picker-ui/src/index.js",
  "module": "./dist/esm/wallet-picker-ui/src/index.js",
  "type": "module",
  "dependencies": {
    "@meer-wallet-selector/core": "workspace:*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "rimraf": "*",
    "typescript": "*"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/wallet-picker-ui/src/index.js",
      "import": "./dist/esm/wallet-picker-ui/src/index.js"
    }
  },
  "keywords": [
    "near wallet",
    "web3",
    "wallet",
    "blockchain",
    "near-protocol",
    "wallet-selector",
    "wallet-picker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedotexe/mallet-selector.git"
  },
  "bugs": {
    "url": "https://github.com/mikedotexe/mallet-selector/issues"
  },
  "homepage": "https://github.com/mikedotexe/mallet-selector/tree/main/packages/wallet-picker-ui"
}
