{
  "name": "@meer-wallet-selector/here-wallet",
  "version": "9.9.9",
  "description": "Here wallet package for NEAR Wallet Selector.",
  "scripts": {
    "compile:esm": "tsc -p tsconfig.esm.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "yarn rimraf dist",
    "publish": "yarn npm publish --access public --tolerate-republish"
  },
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "type": "module",
  "dependencies": {
    "@here-wallet/core": "*",
    "@meer-wallet-selector/core": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/node": "*",
    "jest": "*",
    "rimraf": "*",
    "typescript": "*"
  },
  "files": [
    "./dist/esm",
    "./dist/cjs"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "keywords": [
    "near",
    "blockchain",
    "wallets",
    "dapps",
    "near-protocol",
    "near-blockchain",
    "wallet selector",
    "injected wallet",
    "here wallet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedotexe/mallet-selector.git"
  },
  "bugs": {
    "url": "https://github.com/mikedotexe/mallet-selector/issues"
  },
  "homepage": "https://github.com/mikedotexe/mallet-selector/tree/main/packages/here-wallet"
}
